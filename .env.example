# HTTP存储配置
# 设置存储插件类型为bohrium以启用Bohr存储服务
HTTP_PLUGIN_TYPE=bohrium

# Bohrium存储配置
# 以下配置项根据实际情况设置
BOHRIUM_BOHRIUM_URL=https://bohrium.dp.tech
BOHRIUM_USERNAME=your_username
BOHRIUM_PHONE=your_phone
BOHRIUM_PASSWORD=your_password
BOHRIUM_AUTHORIZATION=your_authorization_token
BOHRIUM_PROJECT_ID=your_project_id
BOHRIUM_TIEFBLUE_URL=https://tiefblue.dp.tech
BOHRIUM_TICKET=your_ticket
BOHRIUM_ACCESS_KEY=your_access_key
BOHRIUM_OPENAPI_URL=https://openapi.dp.tech
BOHRIUM_APP_KEY=agent

# 光子收费配置（可选）
PHOTON_SKU_ID=12345
PHOTON_DEV_ACCESS_KEY=your_dev_access_key
PHOTON_CLIENT_NAME=adk_ui_starter
PHOTON_INPUT_TOKEN_RATE=0.001
PHOTON_OUTPUT_TOKEN_RATE=0.03
PHOTON_TOOL_CALL_COST=1
PHOTON_MIN_CHARGE=1
CHARGING_ENABLED=false
FREE_TOKEN_QUOTA=1000